                    sizes: [300, 250]
                },
            {
                    bids: [{'params': {'tagid': '364655'}, 'bidder': 'sovrn'},
                            {'params': {'placement': 4224512, 'network': '10676.1'}, 'bidder': 'aol'},
                            {'params': {'placementId': 9265111}, 'bidder': 'appnexus'},
