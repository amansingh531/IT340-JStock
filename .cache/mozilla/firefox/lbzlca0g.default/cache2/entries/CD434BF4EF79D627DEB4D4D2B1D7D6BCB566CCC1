                            {'params': {'placementId': 8841124}, 'bidder': 'brealtime'},
                            {'params': {'cp': 558092, 'cf': '728x90', 'ct': '472228'}, 'bidder': 'pulsepoint'},
                            ],
                    code: 'div-gpt-ad-1392148208790-0',
                    sizes: [728, 90]
                }
            ];
            pbjs.addAdUnits(adUnits);

            if (window.performance){
                SF.Ads.bidsRequestTimestamp = performance.now();
            }
            pbjs.requestBids({
                bidsBackHandler:function(){
                    prebid_log('bidsbackhandler calling initAdServer');
                    SF.Ads.initAdServer('bidsBackHandler');
                }
            });
        });

    };

    SF.Ads.debugPrebidResults = function() {
        if (PREBID_DEBUG) {
            var responses = pbjs.getBidResponses();
            var output = [];
            for (var adunit in responses) {
                if (responses.hasOwnProperty(adunit)) {
                    var bids = responses[adunit].bids;
                    for (var i = 0; i < bids.length; i++) {
                        var b = bids[i];
                        var adposition = SF.Ads.Helpers.getPathFromContainerID(b.adUnitCode);
                        output.push({
                            'position': adposition, 'adunit': adunit, 'adId': b.adId, 'bidder': b.bidder,
                            'time': b.timeToRespond, 'cpm': b.cpm, 'msg': b.statusMessage
                        });
                    }
                }
            }
            if (output.length) {
                if (console.table) {
                    console.table(output);
                } else {
                    for (var j = 0; j < output.length; j++) {
                        console.log(output[j]);
                    }
                }
            } else {
                console.warn('NO prebid responses');
            }
        }
    };

    SF.Ads.sendUserTiming = function(category, name, timespanStart){
        if (window.performance) {
            var timeSincePageLoad = performance.now();
            var time = timeSincePageLoad - timespanStart;
            time = Math.round(time);
            ga('send', 'timing', category, name, time);
            console.info('Sent user timing for ' + category + ' ' + name + ' at ' + time);
        }
     }; 
        SF.Ads.initPrebid();
        if (PREBID_DEBUG && performance) {
            performance.mark('prebid-time-start');
            performance.measure('page fetch', 'fetchStart', 'responseEnd');
            performance.measure('page parsing until prebid init', 'responseEnd', 'prebid-time-start');
        }
        setTimeout(function() {
            prebid_log('prebid timeout reached');
            if (PREBID_DEBUG && performance) {
                performance.mark('prebid-time-end');
                performance.measure('prebid-timeout', 'prebid-time-start', 'prebid-time-end');
            }
            SF.Ads.initAdServer('timeout');
        }, PREBID_TIMEOUT);
        prebid_log('prebid init');
    prebid_log('adserver  init');
  </script>



